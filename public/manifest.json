{
  "manifest_version": 2,
  "name": "urlcmd",
  "short_name": "urlcmd",
  "description": "Productivity from your URL bar",
  "version": "0.5.3",
  "permissions": ["storage"],
  "author": "D low",
  "homepage_url": "https://github.com/daniellowtw/urlcmd",
  "background": {
    "scripts": ["js/bg.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "rocket16.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "omnibox": { "keyword": "url" },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+G",
        "mac": "Command+G"
      }
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
