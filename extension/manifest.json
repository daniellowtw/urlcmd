{
    "manifest_version": 2,
    "name": "urlcmd",
    "short_name": "urlcmd",
    "description": "Command line in your url",
    "version": "0.1.0",
    "author": "D low",
    "homepage_url": "https://github.com/daniellowtw/urlcmd",
    "browser_action": {
        "default_title": "UrlCmd",
        "default_popup": "index.html"
    },
    "background": { 
        "page" : "index.html",
        "persistent" : false
    },
    "omnibox": { "keyword": "url" },
    "commands": {
        "shortcut" : {
            "suggested_key" : {
                "default" : "Alt+G"
            },
            "description": "Open the urlcmd page"
        }
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
