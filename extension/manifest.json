{
    "manifest_version": 2,
    "name": "urlcmd",
    "short_name": "urlcmd",
    "description": "Command line in your url",
    "version": "0.1.0",
    "author": "D low",
    "homepage_url": "https://github.com/daniellowtw/urlcmd",
    "browser_action": {
        "default_title": "UrlCmd",
        "default_icon": "go.png", //Used as the icon in the Chrome toolbar 
        "default_popup": "index.html"
    },
    "permissions": [
        "http://localhost:6060/*",
        "https://tip.golang.org/*"
    ],
    "background": { "scripts": ["background.js"] },
    "omnibox": { "keyword": "url" },
    "icons": {
        "16": "go.png"
    }
}
